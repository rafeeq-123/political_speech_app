<div class="username">
<strong> Current User: <%=current_user.email%></strong>
<%all_users%>
<strong> Amount of Users: (<%=@amount_of_parsed_users %>), Help our community grow!</strong>
</div>
<%@speeches.each do |speech| %>
<strong><%=speech.name%></strong>
<strong><%= "Created on"%>  <%=speech.created_at.strftime("%B, %d, %Y")%></strong>
<div class="border_wrapper">
<% if speech.text_of_uploaded_content.nil? %>
  <%= "No content available"%>
<%else%>
  <%= speech.text_of_uploaded_content%>
</div>
<div>
  <label> Here is the analysis:</label><br>
  <%index_political_percent_helper%>

 <%= render partial: "speeches/graph"%>

</div>
  <%end%>
<%end%>


<div class="container text-center">
<%= will_paginate @speeches, renderer: BootstrapPagination::Rails %>
</div>

<%=link_to "Add Comment",edit_speech_path(@speeches.ids), :class =>"btn btn-primary"%>
<%=render partial: 'speeches/all_comments'%>







